<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="PingShu.Entity.Book,PingShu.Entity" table="Book" lazy = "false">

		<id name="BookId" column="Book_ID" type="Int32" unsaved-value="0">
			<generator class="native"/>
		</id>
		<bag name="StoryList" inverse="true" lazy="true" >
			<key column="Book_ID" />
			<one-to-many class="PingShu.Entity.Story,PingShu.Entity" />
		</bag>
		<many-to-one name="ArtistId" column="Artist_ID" class="PingShu.Entity.Artist,PingShu.Entity" />
		<property column="Book_Name" type="String" name="BookName" length="100" />
		<property column="Book_Ico" type="String" name="BookIco" length="100" />
		<property column="Book_Details" type="String" name="BookDetails" length="500" />
		<property column="Book_Read_Count" type="Int32" name="BookReadCount" />
		
	</class>
</hibernate-mapping>
